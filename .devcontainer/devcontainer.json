{
    "name": "Segmentation Model Dev Container",
    "image": "prostai-segmentor:1.5.1-NoDicomOutputs",
    "mounts": [
        "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached"
    ],
    "workspaceFolder": "/workspace",
    "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
    ],
    "remoteUser": "root",
    "settings": {
        "python.pythonPath": "/usr/local/bin/python"
    },
    "mounts": [
        "source=C:/Users/dimza/Desktop/patients_test,target=/Pats,type=bind,consistency=cached",
        "source=C:/Users/dimza/Desktop/patients_test/Outputs,target=/Outputs,type=bind,consistency=cached"
    ]
}